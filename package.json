{
  "name": "rupeez",
  "version": "1.0.0",
  "description": "ATM Locator",
  "license": "MIT",
  "engines": {
    "node": "8.9.1"
  },
  "main": "main.js",
  "scripts": {
    "ng:e2e": "ng e2e",
    "ng:lint": "ng lint",
    "ng:start:dev": "ng serve --env=dev --target=development",
    "ng:start:stg": "ng serve --proxy-config proxy.conf.json --env=stg --target=development",
    "ng:start:prod": "ng serve --ssl true --disable-host-check --host=0.0.0.0 --env=prod --target=production",
    "ng:test": "ng test --watch",
    "ng:test:report": "ng test --no-watch --code-coverage",
    "ng:coveralls": "cat ./coverage/lcov.info | coveralls",
    "ng:doc": "compodoc --customFavicon ./src/favicon.ico --hideGenerator -p src/tsconfig.app.json",
    "ng:build": "ng build --prod --stats-json",
    "ng:build:report": "webpack-bundle-analyzer dist/stats.json",
    "node:start": "nodemon --exec ts-node src/server/main.ts",
    "node:test": "nyc mocha --opts ./mocha.opts --watch --watch-extensions ts",
    "node:test:report": "nyc mocha --opts ./mocha.opts",
    "node:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "node:build": "tsc -p src/tsconfig.server.json",
    "coveralls": "cat coverage/lcov.info node-coverage/lcov.info | coveralls",
    "start": "nodemon --watch 'src' -e 'ts html css' --exec 'npm run ng:build && npm run node:build && node dist/main.js'",
    "compress": "gulp compress",
    "bundle": "cd dist && tar -czvf ../rupeez-${TRAVIS_TAG}.tar.gz * && cd ..",
    "release": "npm version patch && git push --tags origin master",
    "heroku-postbuild": "scripts/heroku-post-install.sh"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "*.spec.ts"
    ],
    "reporter": [
      "lcovonly",
      "text",
      "html"
    ],
    "all": true,
    "cwd": "src/server",
    "report-dir": "node-coverage",
    "temp-directory": "../../node-coverage/.nyc_output"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/platform-server": "^6.0.2",
    "@angular/router": "^6.0.2",
    "@angular/service-worker": "^6.0.2",
    "@google/maps": "^0.4.6",
    "@types/googlemaps": "^3.30.8",
    "ajv": "^6.5.0",
    "core-js": "^2.5.6",
    "express": "^4.16.3",
    "rxjs": "^6.1.0",
    "shrink-ray": "^0.1.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.3",
    "@angular/cli": "6.0.3",
    "@angular/compiler-cli": "^6.0.2",
    "@angular/language-service": "^6.0.2",
    "@compodoc/compodoc": "^1.1.3",
    "@types/chai": "^4.1.3",
    "@types/express": "^4.11.1",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.1.2",
    "autoprefixer": "^8.5.0",
    "chai": "^4.1.2",
    "circular-dependency-plugin": "^5.0.2",
    "codelyzer": "^4.3.0",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.1",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^1.1.11",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.2",
    "gulp-tar": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-coveralls": "^1.2.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "less-loader": "^4.1.0",
    "mocha": "^5.2.0",
    "mock-fs": "^4.5.0",
    "nyc": "^11.8.0",
    "postcss-loader": "^2.1.5",
    "postcss-url": "^7.3.2",
    "protractor": "^5.3.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.1",
    "sinon": "^5.0.7",
    "source-map-explorer": "^1.5.0",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.21.0",
    "stylus-loader": "^3.0.2",
    "ts-node": "^6.0.3",
    "tslint": "~5.10.0",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.13.0",
    "webpack-dev-server": "^3.1.4"
  }
}
